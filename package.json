{
  "name": "advanced-discord-music-bot",
  "version": "2.0.0",
  "description": "An advanced Discord music bot with AI recommendations, karaoke mode, audio effects, and more",
  "main": "bot.js",
  "scripts": {
    "start": "node bot.js",
    "dev": "nodemon bot.js",
    "deploy": "node deploy-commands.js",
    "deploy-guild": "node deploy-commands.js guild",
    "deploy-global": "node deploy-commands.js global",
    "clean-commands": "node deploy-commands.js delete-global",
    "test": "jest",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write .",
    "docker:build": "docker build -t discord-music-bot .",
    "docker:run": "docker run -d --name music-bot discord-music-bot"
  },
  "keywords": [
    "discord",
    "music",
    "bot",
    "ai",
    "karaoke",
    "spotify",
    "youtube",
    "audio-effects",
    "recommendations"
  ],
  "author": "Your Name",
  "license": "MIT",
  "engines": {
    "node": ">=22.0.0",
    "npm": ">=10.0.0"
  },
  "dependencies": {
    "@discordjs/builders": "^1.7.0",
    "@discordjs/voice": "^0.19.0",
    "@distube/ytdl-core": "^4.16.12",
    "@snazzah/davey": "^0.1.8",
    "@supabase/supabase-js": "^2.87.1",
    "axios": "^1.11.0",
    "cheerio": "^1.0.0-rc.12",
    "compression": "^1.8.1",
    "discord.js": "^14.25.1",
    "dotenv": "^16.3.1",
    "ffmpeg-static": "^5.2.0",
    "fluent-ffmpeg": "^2.1.2",
    "genius-lyrics": "^4.4.6",
    "kazagumo": "^3.4.0",
    "libsodium-wrappers": "^0.7.15",
    "lodash": "^4.17.21",
    "moment": "^2.30.1",
    "node-cron": "^3.0.3",
    "opusscript": "^0.0.8",
    "play-dl": "^1.9.7",
    "progress": "^2.0.3",
    "shoukaku": "^4.2.0",
    "spotify-web-api-node": "^5.0.2",
    "tweetnacl": "^1.0.3",
    "uuid": "^9.0.1",
    "youtube-sr": "^4.3.4",
    "ytdl-core": "^4.11.5"
  },
  "devDependencies": {
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.2",
    "prettier": "^3.1.1",
    "supertest": "^6.3.4"
  },
  "optionalDependencies": {
    "@tensorflow/tfjs-node": "^4.15.0",
    "bufferutil": "^4.0.8",
    "prism-media": "^1.3.5",
    "sodium-native": "^4.3.3",
    "utf-8-validate": "^6.0.3"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/advanced-discord-music-bot.git"
  },
  "bugs": {
    "url": "https://github.com/yourusername/advanced-discord-music-bot/issues"
  },
  "homepage": "https://github.com/yourusername/advanced-discord-music-bot#readme",
  "directories": {
    "src": "./src",
    "logs": "./logs",
    "temp": "./temp"
  },
  "config": {
    "ffmpeg_path": "/usr/local/bin/ffmpeg",
    "python_path": "/usr/bin/python3"
  },
  "jest": {
    "testEnvironment": "node",
    "collectCoverage": true,
    "coverageDirectory": "coverage",
    "coveragePathIgnorePatterns": [
      "node_modules/",
      "coverage/"
    ]
  },
  "eslintConfig": {
    "env": {
      "node": true,
      "es2021": true
    },
    "extends": "eslint:recommended",
    "parserOptions": {
      "ecmaVersion": "latest",
      "sourceType": "module"
    },
    "rules": {
      "no-unused-vars": "warn",
      "no-console": "off",
      "prefer-const": "error",
      "no-var": "error"
    }
  },
  "prettier": {
    "semi": true,
    "trailingComma": "es5",
    "singleQuote": true,
    "printWidth": 100,
    "tabWidth": 4,
    "useTabs": false
  },
  "funding": {
    "type": "github",
    "url": "https://github.com/sponsors/yourusername"
  }
}